package app.model.abstract;

class Person {

    name: null,
    surname: {
        name: null,
        surname: null
    },
    age: null,

    Person: function(name, surname){
        this.name = name;
        this.surname = surname;
    }

    Person: function(name, surname, age){
        this.name = name;
        this.surname = surname;
        this.age = age;
    }

    setName: function(name){
        this.name = name;
    }

}


